<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="org.tensorflow.demo.classescheckpet.TelaInicial"
    android:background="#ffffff">

    <!--Logo CheckPet-->
    <ImageView
        android:id="@+id/LogoCheckPet"
        android:layout_width="320dp"
        android:layout_height="160dp"
        android:layout_above="@id/LogoLabvis"
        android:layout_marginBottom="10dp"
        android:layout_centerHorizontal="true"
        android:scaleType="fitCenter"
        android:src="@drawable/logo_checkpet"/>

    <!--Logo LABVIS-->
    <ImageView
        android:id="@+id/LogoLabvis"
        android:layout_width="160dp"
        android:layout_height="160dp"
        android:layout_above="@id/LogoUfpa"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:scaleType="fitCenter"
        android:src="@drawable/labvis"/>

    <!--Logo UFPA-->
    <ImageView
        android:id="@+id/LogoUfpa"
        android:layout_width="170dp"
        android:layout_height="170dp"
        android:layout_above="@id/CopyrightText"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:scaleType="fitCenter"
        android:src="@drawable/logo_ufpa_menor"/>


    <!--Logo Copyright-->
    <TextView
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:id="@+id/CopyrightText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textSize="20dp"
        android:text="Copyright Â© 2017"/>


</RelativeLayout>